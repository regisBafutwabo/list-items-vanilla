ocument.querySelectorAll('li');\n\n    li.addEventListener('click', (event) => {\n        const body = document.body;\n        const clickedItem = event.target;\n        const modal = document.querySelector('.modal');\n        const modalContent = document.getElementsByClassName()\n\n        modal.style.display='block';\n\n        card.textContent = clickedItem.textContent;\n        body.appendChild(card); \n\n        // Center the card view\n        const screenWidth = window.innerWidth;\n        const screenHeight = window.innerHeight;\n        console.log(screenWidth);\n        const cardTop = (screenHeight - listItemHeight) / 2;\n        const cardLeft = (screenWidth - listItemWidth) / 2;\n        card.style.top = `${cardTop}px`;\n        card.style.left = `${cardLeft}px`;\n\n        // Resize the clicked item to pop out\n        clickedItem.style.transform = 'scale(1.2)';\n        clickedItem.style.zIndex = '2';\n        clickedItem.style.transition = 'transform 0.2s ease-in-out';\n\n        // Add a dimmed background\n        const dimmedBg = document.createElement('div');\n        dimmedBg.classList.add('dimmed-bg');\n        body.appendChild(dimmedBg);\n\n        // Restore to the list view when dimmed background is clicked\n        dimmedBg.addEventListener('click', () => {\n            clickedItem.style.transform = 'none';\n            clickedItem.style.zIndex = '1';\n            clickedItem.style.transition = 'transform 0.2s ease-in-out';\n            body.removeChild(card);\n            body.removeChild(dimmedBg);\n        });\n    });\n    "},"lineCount":null}},"error":null,"hash":"000c74e3d5dd5c6c7063eeff14384a46","cacheData":{"env":{}}}