iginal":{"line":52,"column":13}}],"sources":{"js/card.js":"export function onItemClick(){\n    const listItem = document.querySelectorAll('.list-item');\n    console.log(\"LIST size\", listItem.length)\n    if(listItem.length>0 ){\n        listItem.forEach(item => {\n            item.addEventListener('click', (event) => {\n                const clickedItem = event.target;\n                const listItemRect = clickedItem.getBoundingClientRect();\n                const listItemTop = listItemRect.top;\n                const listItemLeft = listItemRect.left;\n                const listItemWidth = listItemRect.width;\n                const listItemHeight = listItemRect.height;\n\n                // Create a card view element\n                const card = document.createElement('div');\n                card.classList.add('card');\n                card.textContent = clickedItem.textContent;\n                body.appendChild(card);\n\n                // Center the card view\n                const screenWidth = window.innerWidth;\n                const screenHeight = window.innerHeight;\n                const cardTop = (screenHeight - listItemHeight) / 2;\n                const cardLeft = (screenWidth - listItemWidth) / 2;\n                card.style.top = `${cardTop}px`;\n                card.style.left = `${cardLeft}px`;\n\n                // Resize the clicked item to pop out\n                clickedItem.style.transform = 'scale(1.2)';\n                clickedItem.style.zIndex = '2';\n                clickedItem.style.transition = 'transform 0.2s ease-in-out';\n\n                // Add a dimmed background\n                const dimmedBg = document.createElement('div');\n                dimmedBg.classList.add('dimmed-bg');\n                body.appendChild(dimmedBg);\n\n                // Restore to the list view when dimmed background is clicked\n                dimmedBg.addEventListener('click', () => {\n                    clickedItem.style.transform = 'none';\n                    clickedItem.style.zIndex = '1';\n                    clickedItem.style.transition = 'transform 0.2s ease-in-out';\n                    body.removeChild(card);\n                    body.removeChild(dimmedBg);\n            \n                });\n            });\n        })\n    }\n}\n    \nonItemClick();"},"lineCount":null}},"error":null,"hash":"541fd879da43933d1c0a159b66496f8f","cacheData":{"env":{}}}